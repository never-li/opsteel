@import '/static/css/common.wxss';
.operate-wrap .wrap-tab {
  height: 85rpx;
  border-bottom: 2rpx solid #E8E8E8;
}
.operate-wrap .wrap-tab .tab {
  font-size: 32rpx;
  line-height: 64rpx;
  color: #333333;
  text-align: center;
}
.operate-wrap .wrap-tab .tab.current {
  position: relative;
  color: #008CFF;
}
.operate-wrap .wrap-tab .tab.current::after {
  content: '';
  position: absolute;
  bottom: -10rpx;
  left: 10%;
  width: 80%;
  border: 3rpx solid #008CFF;
}
.operate-wrap .wrap-transaction .deal {
  height: 225rpx;
}
.operate-wrap .wrap-transaction .deal .today {
  display: inline-block;
  margin-top: 29rpx;
  margin-bottom: 14rpx;
  padding-left: 8rpx;
  padding-right: 8rpx;
  font-size: 24rpx;
  line-height: 36rpx;
  height: 36rpx;
  background: #F95355;
  color: #FFFFFF;
  border-radius: 0 5rpx 5rpx 0;
}
.operate-wrap .wrap-transaction .deal .data .txt {
  text-align: center;
}
.operate-wrap .wrap-transaction .deal .data .txt.line {
  position: relative;
}
.operate-wrap .wrap-transaction .deal .data .txt.line::after {
  content: '';
  position: absolute;
  top: 18rpx;
  right: 0;
  width: 1px;
  height: 70rpx;
  border-right: 1rpx solid #E8E8E8;
}
.operate-wrap .wrap-transaction .deal .data .txt .p1 {
  display: block;
  overflow: hidden;
  font-size: 26rpx;
  height: 65rpx;
  color: #F95355;
}
.operate-wrap .wrap-transaction .deal .data .txt .p1 span {
  font-size: 46rpx;
}
.operate-wrap .wrap-transaction .deal .data .txt .p2 {
  font-size: 24rpx;
  color: #999999;
}
.operate-wrap .wrap-line {
  width: 100%;
  height: 20rpx;
  background: #f5f5f5;
}
.operate-wrap .wrap-chart {
  padding: 34rpx 24rpx 0 24rpx;
}
.operate-wrap .wrap-chart .chart-title {
  position: relative;
  font-size: 28rpx;
  color: #333333;
  padding-left: 16rpx;
  font-weight: 700;
}
.operate-wrap .wrap-chart .chart-title::after {
  position: absolute;
  bottom: 5rpx;
  left: 0;
  content: '';
  width: 6rpx;
  height: 28rpx;
  background: #008CFF;
}
.operate-wrap .wrap-chart .chart-options {
  position: relative;
  width: 180rpx;
  font-size: 26rpx;
  color: #666666;
}
.operate-wrap .wrap-chart .chart-options .options-selected {
  display: inline-block;
  line-height: 70rpx;
  height: 70rpx;
}
.operate-wrap .wrap-chart .chart-options .options-team {
  position: absolute;
  width: 180rpx;
  bottom: -120rpx;
  left: 0;
}
.operate-wrap .wrap-chart .chart-options .options-team li {
  display: block;
  line-height: 45rpx;
}
.operate-wrap .wrap-chart .K-container {
  width: 100%;
  height: 310rpx;
  margin-bottom: 30rpx;
}
.operate-wrap .wrap-chart .pic-container {
  width: 100%;
  height: 280rpx;
}
.operate-wrap .wrap-chart .bar-container {
  width: 100%;
  height: 400rpx;
  margin-bottom: 30rpx;
}
.operate-wrap .wrap-chart .map-container {
  width: 100%;
  height: 600rpx;
}
.operate-wrap .wrap-chart .line-container {
  width: 100%;
  height: 320rpx;
  margin-bottom: 30rpx;
}
.operate-wrap .wrap-chart ec-canvas {
  width: 100%;
  height: 100%;
}
.operate-wrap .wrap-chart .table {
  margin-top: 36rpx;
  margin-bottom: 30rpx;
  border: 2rpx solid #E8E8E8;
}
.operate-wrap .wrap-chart .table .table-heed {
  font-size: 24rpx;
  color: #333333;
  line-height: 60rpx;
  height: 60rpx;
  background: #F2F9FC;
}
.operate-wrap .wrap-chart .table .table-heed view {
  border-top: none;
}
.operate-wrap .wrap-chart .table .table-heed view:last-child {
  border-right: none;
}
.operate-wrap .wrap-chart .table .table-body view {
  position: relative;
}
.operate-wrap .wrap-chart .table .table-body view image {
  position: absolute;
  left: 10rpx;
  top: 50%;
  margin-top: -16rpx;
  width: 26rpx;
  height: 32rpx;
}
.operate-wrap .wrap-chart .table .table-body view:first-child {
  text-align: left;
}
.operate-wrap .wrap-chart .table .table-body view:first-child .name {
  vertical-align: bottom;
  display: inline-block;
  width: 190rpx;
  font-size: 24rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin-left: 40rpx;
}
.operate-wrap .wrap-chart .table .table-body view.tc {
  text-align: center;
}
.operate-wrap .wrap-chart .table .table-body view:last-child {
  border-right: none;
}
.operate-wrap .wrap-chart .table view > view {
  width: 33.3%;
  font-size: 24rpx;
  line-height: 60rpx;
  box-sizing: border-box;
  text-align: center;
  color: #333333;
  border-top: 2rpx solid #E8E8E8;
  border-right: 2rpx solid #E8E8E8;
}
.operate-wrap .Leaderboard .subtitle {
  margin-top: 80rpx;
  margin-bottom: 25rpx;
  font-size: 24rpx;
  color: #333333;
  text-align: center;
}
.operate-wrap .Leaderboard .table {
  display: inline-block;
  margin: 0 0rpx 30rpx 0rpx;
  border: 1rpx solid #E8E8E8;
}
.operate-wrap .Leaderboard .table li {
  display: block;
}
.operate-wrap .Leaderboard .table li span {
  display: inline-block;
  text-align: center;
  line-height: 60rpx;
}
.operate-wrap .Leaderboard .table .table-title {
  font-size: 24rpx;
  color: #333333;
  background: #F2F9FC;
}
.operate-wrap .Leaderboard .table .table-title .rank {
  border-top: none;
}
.operate-wrap .Leaderboard .table .table-title .area {
  border-top: none;
}
.operate-wrap .Leaderboard .table .table-title .amount {
  border-top: none;
}
.operate-wrap .Leaderboard .table .rank {
  width: 120rpx;
  height: 60rpx;
  border-right: 1rpx solid #E8E8E8;
  border-top: 1rpx solid #E8E8E8;
}
.operate-wrap .Leaderboard .table .area {
  width: 285rpx;
  height: 60rpx;
  border-right: 1rpx solid #E8E8E8;
  border-top: 1rpx solid #E8E8E8;
}
.operate-wrap .Leaderboard .table .amount {
  width: 285rpx;
  height: 60rpx;
  border-top: 1rpx solid #E8E8E8;
}
.operate-wrap .wrap-user .user-data {
  padding: 50rpx 24rpx;
}
.operate-wrap .wrap-user .user-data .data-item {
  position: relative;
  text-align: center;
}
.operate-wrap .wrap-user .user-data .data-item .num {
  display: inline-block;
  font-size: 46rpx;
  color: #F95355;
  line-height: 65rpx;
  margin-bottom: 7rpx;
  max-width: 200rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.operate-wrap .wrap-user .user-data .data-item .num span {
  font-size: 35rpx;
}
.operate-wrap .wrap-user .user-data .data-item .txt {
  font-size: 24rpx;
  color: #999999;
  line-height: 33rpx;
}
.operate-wrap .wrap-user .user-data .data-item::after {
  content: '';
  position: absolute;
  top: 50%;
  right: 0;
  width: 0;
  height: 70rpx;
  border-right: 2rpx solid #E8E8E8;
  margin-top: -35rpx;
}
.operate-wrap .wrap-user .user-data .data-item:last-child::after {
  border-right: none;
}
.operate-wrap .wrap-user .user-data .data-item:last-child .num {
  color: #666666;
}
