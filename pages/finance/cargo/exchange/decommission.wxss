@import '/static/css/common.wxss';
.decommission-wrap {
  width: 100%;
  background: #FFFFFF !important;
}
.decommission-wrap .wrap-head {
  padding: 18rpx 24rpx;
  background: #FAFAFA;
  font-size: 24rpx;
  color: #666666;
}
.decommission-wrap .wrap-head b {
  color: #F95355;
}
.decommission-wrap .wrap-title {
  font-size: 26rpx;
  color: #666666;
  line-height: 37rpx;
  padding: 20rpx 0 23rpx 0;
  margin-left: 24rpx;
  border-bottom: 2rpx solid #E8E8E8;
}
.decommission-wrap .wrap-line {
  width: 100%;
  height: 20rpx;
  background: #F5F5F5;
}
.decommission-wrap .wrap-total {
  background: #FFFFFF;
  font-size: 26rpx;
  padding: 20rpx 0;
  margin: 0 24rpx;
  color: #666666;
}
.decommission-wrap .wrap-total .total {
  color: #F95355;
}
