@import '/static/css/common.wxss';
.confirmMessage-wrap .wrap-title {
  font-size: 26rpx;
  color: #333333;
  margin-left: 24rpx;
  padding: 24rpx 0;
  margin-bottom: 26rpx;
  font-weight: 700;
  border-bottom: 2rpx solid #E8E8E8;
}
.confirmMessage-wrap .wrap-box {
  margin-left: 24rpx;
}
.confirmMessage-wrap .wrap-box .item {
  padding-bottom: 13rpx;
  margin-bottom: 26rpx;
  border-bottom: 2rpx solid #E8E8E8;
}
.confirmMessage-wrap .wrap-box .item > view {
  width: 50%;
  display: inline-block;
  font-size: 24rpx;
  color: #333333;
  line-height: 33rpx;
  margin-bottom: 13rpx;
}
.confirmMessage-wrap .wrap-box .item > view text {
  display: inline-block;
  width: 96rpx;
  color: #999999;
  margin-right: 14rpx;
}
.confirmMessage-wrap .wrap-box .item > view span {
  font-size: 20rpx;
  color: #F95355;
}
.confirmMessage-wrap .wrap-box .item > view b {
  color: #F95355;
}
.confirmMessage-wrap .wrap-total {
  font-size: 26rpx;
  line-height: 40rpx;
  color: colorGray2;
  text-align: right;
  padding: 20rpx 0;
  margin: 0 24rpx;
}
.confirmMessage-wrap .wrap-total b {
  color: #F95355;
}
.confirmMessage-wrap .wrap-line {
  width: 100%;
  height: 20rpx;
  background: #F5F5F5;
}
